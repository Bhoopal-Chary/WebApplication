@model Register

<div class="container">
    <div class="row pt-4 pb-3">
        <div class="col-6">
            <h2>
                Delete Employee Details

            </h2>
        </div>
    </div>
</div>
<form method="post">
    <input asp-for="EmployeeId" hidden />



    <div data-mdb-input-init class="form-outline mb-4">
        <input asp-for="Username" disabled type="text" id="form3Example1q" class="form-control" />
        <label asp-for="Username" class="form-label" for="form3Example1q"></label>
    </div>

    <div data-mdb-input-init class="form-outline mb-4">
        <input asp-for="Password" disabled type="password" id="form3Example1q" class="form-control" />
        <label asp-for="Password" class="form-label" for="form3Example1q"></label>
    </div>

    <div data-mdb-input-init class="form-outline mb-4">
        <input asp-for="Email" disabled type="text" id="form3Example1q" class="form-control" />
        <label asp-for="Email" class="form-label" for="form3Example1q"></label>
    </div>

    
    <div class="d-flex">
    <button type="submit"  class="btn btn-outline-danger ">
        Delete

    </button>
    <div class="col-1 col-md-1">
        <a asp-controller="Account" asp-action="EmployeeDetails" class="btn btn-outline-dark">Go Back</a>
    </div>
    </div>
</form>
                   
